{
    "manifestVersion": 1,
    "id": "JingetQueryRunner",
    "name": "Jinget Query Runner",
    "version": "1.0.5",
    "publisher": "VahidFarahmandian",
    "description": "Using this extension, you can run various queries on SQL Server databases",
    "public": true,
    "branding": {
        "color": "rgb(1, 10, 37)",
        "theme": "dark"
    },
    "links": {
        "home": {
            "uri": "https://jinget.ir"
        },
        "repository": {
            "uri": "https://github.com/VahidFarahmandian/jinget-azure-sqlserver-query-runner"
        },
        "issues": {
            "uri": "https://github.com/VahidFarahmandian/jinget-azure-sqlserver-query-runner/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/VahidFarahmandian/jinget-azure-sqlserver-query-runner"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "sql server",
        "sql",
        "query"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "Task"
        }
    ],
    "contributions": [
        {
            "id": "JingetQueryRunner",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Task"
            }
        }
    ],
    "screenshots": [
        {
            "path": "screenshots/1.png"
        },
        {
            "path": "screenshots/2.png"
        },
        {
            "path": "screenshots/3.png"
        },
        {
            "path": "screenshots/4.png"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    }
}